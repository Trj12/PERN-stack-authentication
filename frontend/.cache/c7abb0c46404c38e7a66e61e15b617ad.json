/Auth.js":"import auth0, {\n    WebAuth\n} from 'auth0-js';\nimport history from './history';\nclass Auth {\n    auth0 = new WebAuth({\n        domain: 'react-auth-project.auth0.com',\n        clientID: 'ygL926gSZk9jyn6wF6gNwU8td42QyQ2V',\n        redirectUri: 'http://localhost:1234/callback',\n        responseType: 'token'\n    });\n\n    login = () => {\n        this.auth0.authorize();\n    }\n\n    handelAuthentication = () => {\n        this.auth0.parseHash((err,authRes)=>{\n            if(authRes){\n                localStorage.setItem('access_token',authRes.accessToken);\n                localStorage.setItem('expires_at',JSON.stringify(authRes.expiresIn * 1000 + new Date().getTime()))\n                this.loggedIn=true;\n                history.replace('/');\n            }else if(err){\n                console.log('err',err);\n            }\n        });\n    }\n\n    logout = () => {\n        ['access_token','expires_at'].forEach(item=>localStorage.removeItem(item));\n        this.loggedIn=false;\n        history.replace('/');\n    }\n    isAuthenticated =()=> {\n      \n        return  new Date().getTime() < +localStorage.getItem('expires_at');\n    }\n\n}\nexport default Auth"},"lineCount":null}},"hash":"6bac0b8381fe084674895dddb4cb855d","cacheData":{"env":{}}}